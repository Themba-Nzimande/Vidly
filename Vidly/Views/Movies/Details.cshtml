@model Vidly.Models.Movie

@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<h3>Movie details</h3>
<ul>
    <li>Genre: @Model.Genre.GenreName</li>
    <li>Released Date: @Model.ReleaseDate.ToShortDateString()</li>
    <li>Number in Stock: @Model.NumberInStock.ToString()</li>

</ul>


